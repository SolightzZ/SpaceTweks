{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.vindicator": {
      "geometry": "Geometry.default",
      "part_visibility": [
        {
          "rightArm": "query.is_angry || query.is_celebrating"
        },
        {
          "leftArm": "query.is_angry || query.is_celebrating"
        },
        {
          "rightItem": "query.is_angry"
        },
        {
          "arms": "!(query.is_angry || query.is_celebrating)"
        }
      ],
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": [
        "Array.Xskins[variable.Xvar]"
      ],
      "arrays": {
        "textures": {
          "Array.Xskins": [
            "Texture.default",
            "Texture.default2",
            "Texture.default3",
            "Texture.default4",
            "Texture.default5",
            "Texture.default6"
          ]
        }
      }
    }
  }
}
